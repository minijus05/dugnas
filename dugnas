class TokenPriceMonitor:
    def __init__(self):
        self.client = TelegramClient('session_name', TELEGRAM_API_ID, TELEGRAM_API_HASH)
        self.sent_signals: Dict[str, datetime] = {}
        self.session = None
        self.monitoring_tokens = set()
